{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nexport default {\n  name: \"Manager\",\n  // 组件名称\n  data() {\n    return {\n      // 从 localStorage 获取用户信息，若不存在则返回空对象\n      user: JSON.parse(localStorage.getItem('xm-user') || '{}')\n    };\n  },\n  created() {\n    // 当组件被创建时检查用户是否登录\n    if (!this.user.id) {\n      // 若用户未登录，则跳转到登录页面\n      this.$router.push('/login');\n    }\n  },\n  methods: {\n    // 更新用户信息\n    updateUser() {\n      // 从 localStorage 重新获取最新的用户数据\n      this.user = JSON.parse(localStorage.getItem('xm-user') || '{}');\n    },\n    // 跳转到对应角色的个人信息页面\n    goToPerson() {\n      if (this.user.role === 'ADMIN') {\n        this.$router.push('/adminPerson');\n      } else if (this.user.role === 'LABADMIN') {\n        this.$router.push('/labAdminPerson');\n      } else if (this.user.role === 'TEACHER') {\n        this.$router.push('/teacherPerson');\n      } else if (this.user.role === 'STUDENT') {\n        this.$router.push('/studentPerson');\n      }\n    },\n    // 退出登录\n    logout() {\n      // 清除本地存储中的用户信息\n      localStorage.removeItem('xm-user');\n      // 跳转到登录页面\n      this.$router.push('/login');\n    }\n  }\n};","map":{"version":3,"names":["name","data","user","JSON","parse","localStorage","getItem","created","id","$router","push","methods","updateUser","goToPerson","role","logout","removeItem"],"sources":["src/views/Manager.vue"],"sourcesContent":["<template>\r\n  <div class=\"manager-container\">\r\n    <!-- 头部区域 -->\r\n    <div class=\"manager-header\">\r\n      <!-- 左侧部分：Logo 和系统标题 -->\r\n      <div class=\"manager-header-left\">\r\n        <img class=\"logo\" src=\"@/assets/imgs/logo.png\" alt=\"Logo\" />\r\n        <div class=\"title\">实验室管理系统</div>\r\n      </div>\r\n\r\n      <!-- 中间部分：面包屑导航 -->\r\n      <div class=\"manager-header-center\">\r\n        <el-breadcrumb separator-class=\"el-icon-arrow-right\">\r\n          <!-- 第一项固定为首页 -->\r\n          <el-breadcrumb-item :to=\"{ path: '/' }\">首页</el-breadcrumb-item>\r\n          <!-- 第二项根据当前路由动态更新 -->\r\n          <el-breadcrumb-item :to=\"{ path: $route.path }\">{{ $route.meta.name }}</el-breadcrumb-item>\r\n        </el-breadcrumb>\r\n      </div>\r\n\r\n      <!-- 右侧部分：用户信息及操作菜单 -->\r\n      <div class=\"manager-header-right\">\r\n        <el-dropdown placement=\"bottom\">\r\n          <div class=\"avatar\">\r\n            <!-- 显示用户头像，若没有头像则使用默认图片 -->\r\n            <img :src=\"user.avatar || 'https://cube.elemecdn.com/3/7c/3ea6beec64369c2642b92c6726f1epng.png'\" />\r\n            <!-- 显示用户名称，若未提供则显示“管理员” -->\r\n            <div>{{ user.name || '管理员' }}</div>\r\n          </div>\r\n          <!-- 用户操作菜单 -->\r\n          <el-dropdown-menu slot=\"dropdown\">\r\n            <el-dropdown-item @click.native=\"goToPerson\">个人信息</el-dropdown-item>\r\n            <el-dropdown-item @click.native=\"$router.push('/password')\">修改密码</el-dropdown-item>\r\n            <el-dropdown-item @click.native=\"logout\">退出登录</el-dropdown-item>\r\n          </el-dropdown-menu>\r\n        </el-dropdown>\r\n      </div>\r\n    </div>\r\n\r\n    <!-- 主体区域 -->\r\n    <div class=\"manager-main\">\r\n      <!-- 左侧侧边栏 -->\r\n      <div class=\"manager-main-left\">\r\n        <el-menu\r\n            :default-openeds=\"['info', 'user']\"\r\n            router\r\n            style=\"border: none\"\r\n            :default-active=\"$route.path\"\r\n        >\r\n          <!-- 系统首页 -->\r\n          <el-menu-item index=\"/home\">\r\n            <i class=\"el-icon-s-home\"></i>\r\n            <span slot=\"title\">系统首页</span>\r\n          </el-menu-item>\r\n\r\n          <!-- 信息管理菜单 -->\r\n          <el-submenu index=\"info\">\r\n            <template slot=\"title\">\r\n              <i class=\"el-icon-menu\"></i><span>信息管理</span>\r\n            </template>\r\n            <!-- 根据角色动态显示不同的菜单项 -->\r\n            <el-menu-item index=\"/semester\" v-if=\"user.role === 'ADMIN'\">学期信息</el-menu-item>\r\n            <el-menu-item index=\"/lab\" v-if=\"user.role === 'ADMIN'\">实验室信息</el-menu-item>\r\n            <el-menu-item index=\"/doBorrow\" v-if=\"user.role === 'ADMIN'\">实验室借用审批</el-menu-item>\r\n            <el-menu-item index=\"/doSchedule\" v-if=\"user.role === 'ADMIN'\">实验室排课审批</el-menu-item>\r\n            <el-menu-item index=\"/doRepair\" v-if=\"user.role === 'LABADMIN'\">设备报修处理</el-menu-item>\r\n            <el-menu-item index=\"/schedule\" v-if=\"user.role === 'TEACHER'\">实验课申请登记</el-menu-item>\r\n            <el-menu-item index=\"/repair\" v-if=\"user.role === 'TEACHER'\">实验室设备报修</el-menu-item>\r\n            <el-menu-item index=\"/borrow\" v-if=\"user.role === 'STUDENT'\">实验室借用申请</el-menu-item>\r\n          </el-submenu>\r\n\r\n          <!-- 用户管理菜单（仅管理员可见） -->\r\n          <el-submenu index=\"user\" v-if=\"user.role === 'ADMIN'\">\r\n            <template slot=\"title\">\r\n              <i class=\"el-icon-menu\"></i><span>用户管理</span>\r\n            </template>\r\n            <el-menu-item index=\"/admin\">管理员信息</el-menu-item>\r\n            <el-menu-item index=\"/labAdmin\">实验员信息</el-menu-item>\r\n            <el-menu-item index=\"/teacher\">教师信息</el-menu-item>\r\n            <el-menu-item index=\"/student\">学生信息</el-menu-item>\r\n          </el-submenu>\r\n        </el-menu>\r\n      </div>\r\n\r\n      <!-- 右侧主内容区域 -->\r\n      <div class=\"manager-main-right\">\r\n        <!-- 渲染当前路由对应的组件 -->\r\n        <router-view @update:user=\"updateUser\" />\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: \"Manager\", // 组件名称\r\n  data() {\r\n    return {\r\n      // 从 localStorage 获取用户信息，若不存在则返回空对象\r\n      user: JSON.parse(localStorage.getItem('xm-user') || '{}'),\r\n    };\r\n  },\r\n  created() {\r\n    // 当组件被创建时检查用户是否登录\r\n    if (!this.user.id) {\r\n      // 若用户未登录，则跳转到登录页面\r\n      this.$router.push('/login');\r\n    }\r\n  },\r\n  methods: {\r\n    // 更新用户信息\r\n    updateUser() {\r\n      // 从 localStorage 重新获取最新的用户数据\r\n      this.user = JSON.parse(localStorage.getItem('xm-user') || '{}');\r\n    },\r\n    // 跳转到对应角色的个人信息页面\r\n    goToPerson() {\r\n      if (this.user.role === 'ADMIN') {\r\n        this.$router.push('/adminPerson');\r\n      } else if (this.user.role === 'LABADMIN') {\r\n        this.$router.push('/labAdminPerson');\r\n      } else if (this.user.role === 'TEACHER') {\r\n        this.$router.push('/teacherPerson');\r\n      } else if (this.user.role === 'STUDENT') {\r\n        this.$router.push('/studentPerson');\r\n      }\r\n    },\r\n    // 退出登录\r\n    logout() {\r\n      // 清除本地存储中的用户信息\r\n      localStorage.removeItem('xm-user');\r\n      // 跳转到登录页面\r\n      this.$router.push('/login');\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n@import \"@/assets/css/manager.css\"; /* 引入全局样式表 */\r\n\r\n/* Logo 样式 */\r\n.logo {\r\n  max-width: 100%;\r\n  height: auto;\r\n  object-fit: contain;\r\n}\r\n</style>\r\n"],"mappings":";AA8FA;EACAA,IAAA;EAAA;EACAC,KAAA;IACA;MACA;MACAC,IAAA,EAAAC,IAAA,CAAAC,KAAA,CAAAC,YAAA,CAAAC,OAAA;IACA;EACA;EACAC,QAAA;IACA;IACA,UAAAL,IAAA,CAAAM,EAAA;MACA;MACA,KAAAC,OAAA,CAAAC,IAAA;IACA;EACA;EACAC,OAAA;IACA;IACAC,WAAA;MACA;MACA,KAAAV,IAAA,GAAAC,IAAA,CAAAC,KAAA,CAAAC,YAAA,CAAAC,OAAA;IACA;IACA;IACAO,WAAA;MACA,SAAAX,IAAA,CAAAY,IAAA;QACA,KAAAL,OAAA,CAAAC,IAAA;MACA,gBAAAR,IAAA,CAAAY,IAAA;QACA,KAAAL,OAAA,CAAAC,IAAA;MACA,gBAAAR,IAAA,CAAAY,IAAA;QACA,KAAAL,OAAA,CAAAC,IAAA;MACA,gBAAAR,IAAA,CAAAY,IAAA;QACA,KAAAL,OAAA,CAAAC,IAAA;MACA;IACA;IACA;IACAK,OAAA;MACA;MACAV,YAAA,CAAAW,UAAA;MACA;MACA,KAAAP,OAAA,CAAAC,IAAA;IACA;EACA;AACA","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}