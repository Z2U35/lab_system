{"ast":null,"code":"export default {\n  name: \"Borrow\",\n  data() {\n    return {\n      tableData: [],\n      // 表格数据\n      pageNum: 1,\n      // 当前页码\n      pageSize: 10,\n      // 每页显示的条数\n      total: 0,\n      // 总记录数\n      fromVisible: false,\n      // 控制对话框显示\n      form: {},\n      // 表单数据\n      user: JSON.parse(localStorage.getItem('xm-user') || '{}'),\n      // 当前用户信息\n      curSemester: null,\n      // 当前学期信息\n      rules: {\n        // 表单验证规则\n        labId: [{\n          required: true,\n          message: '请选择实验室编号',\n          trigger: 'blur'\n        }],\n        week: [{\n          required: true,\n          message: '请选择借用周',\n          trigger: 'blur'\n        }],\n        day: [{\n          required: true,\n          message: '请选择借用星期',\n          trigger: 'blur'\n        }],\n        sessionId: [{\n          required: true,\n          message: '请选择借用节次',\n          trigger: 'blur'\n        }],\n        reason: [{\n          required: true,\n          message: '请输入借用原因',\n          trigger: 'blur'\n        }]\n      },\n      labData: [],\n      // 实验室数据\n      sessions: [],\n      // 节次数据\n      headFilters: [] // 筛选器配置\n    };\n  },\n  created() {\n    // 页面加载时初始化数据\n    this.load(1); // 加载表格数据\n    this.loadLab(); // 加载实验室信息\n    this.loadCurSemester(); // 加载当前学期信息\n    this.loadSessions(); // 加载节次信息\n  },\n  methods: {\n    load(pageNum) {\n      // 分页加载数据\n      if (pageNum) this.pageNum = pageNum;\n      this.$request.get('/borrow/selectPage', {\n        params: {\n          pageNum: this.pageNum,\n          pageSize: this.pageSize\n        }\n      }).then(res => {\n        if (res.code === '200') {\n          this.tableData = res.data?.list;\n          this.total = res.data?.total;\n        }\n      });\n    },\n    handleAdd() {\n      // 打开新增借用申请对话框\n      this.form = {};\n      this.fromVisible = true;\n    },\n    save() {\n      // 保存数据\n      this.$refs.formRef.validate(valid => {\n        if (valid) {\n          this.$request.post('/borrow/add', this.form).then(res => {\n            if (res.code === '200') {\n              this.$message.success('保存成功');\n              this.load(1);\n              this.fromVisible = false;\n            }\n          });\n        }\n      });\n    }\n  }\n};","map":{"version":3,"names":["name","data","tableData","pageNum","pageSize","total","fromVisible","form","user","JSON","parse","localStorage","getItem","curSemester","rules","labId","required","message","trigger","week","day","sessionId","reason","labData","sessions","headFilters","created","load","loadLab","loadCurSemester","loadSessions","methods","$request","get","params","then","res","code","list","handleAdd","save","$refs","formRef","validate","valid","post","$message","success"],"sources":["src/views/manager/Borrow.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <!-- 操作区 -->\r\n    <div class=\"operation\">\r\n      <el-button type=\"primary\" plain @click=\"handleAdd\">借用申请</el-button> <!-- 点击触发新增申请 -->\r\n    </div>\r\n\r\n    <!-- 表格展示 -->\r\n    <div class=\"table\">\r\n      <el-table :data=\"tableData\" strip @selection-change=\"handleSelectionChange\">\r\n        <!-- 表格列定义 -->\r\n        <el-table-column prop=\"id\" label=\"序号\" width=\"70\" align=\"center\" sortable></el-table-column>\r\n        <el-table-column prop=\"labNumber\" label=\"实验室编号\"  :filters=\"headFilters['labNumber']\" :filter-method=\"filterHandler\"></el-table-column>\r\n        <el-table-column prop=\"semesterName\" label=\"学期\" sortable></el-table-column>\r\n        <el-table-column prop=\"week\" label=\"周次\"></el-table-column>\r\n        <el-table-column prop=\"day\" label=\"星期\"></el-table-column>\r\n        <el-table-column prop=\"sessionName\" label=\"节次\"></el-table-column>\r\n        <el-table-column prop=\"studentName\" label=\"申请学生\"></el-table-column>\r\n        <el-table-column prop=\"reason\" label=\"申请原因\"></el-table-column>\r\n        <el-table-column prop=\"applyDate\" label=\"申请日期\" sortable></el-table-column>\r\n        <el-table-column prop=\"status\" label=\"状态\"  :filters=\"headFilters['status']\" :filter-method=\"filterHandler\"></el-table-column>\r\n\r\n        <!-- 操作列 -->\r\n        <el-table-column label=\"操作\" align=\"center\" width=\"180\">\r\n          <template v-slot=\"scope\">\r\n            <!-- 根据状态显示不同的操作按钮 -->\r\n            <el-button v-if=\"scope.row.status === '未审核' || scope.row.status === '驳回'\" size=\"mini\" type=\"primary\" plain @click=\"handleEdit(scope.row)\">编辑</el-button>\r\n            <el-button v-if=\"scope.row.status === '未审核' || scope.row.status === '驳回'\" size=\"mini\" type=\"danger\" plain @click=\"del(scope.row.id)\">撤回</el-button>\r\n            <el-button v-if=\"scope.row.status === '通过'\" size=\"mini\" type=\"success\" plain @click=\"finish(scope.row)\">确认</el-button>\r\n          </template>\r\n        </el-table-column>\r\n      </el-table>\r\n\r\n      <!-- 分页器 -->\r\n      <div class=\"pagination\">\r\n        <el-pagination\r\n            background\r\n            @current-change=\"handleCurrentChange\"\r\n            :current-page=\"pageNum\"\r\n            :page-sizes=\"[5, 10, 20]\"\r\n            :page-size=\"pageSize\"\r\n            layout=\"total, prev, pager, next\"\r\n            :total=\"total\">\r\n        </el-pagination>\r\n      </div>\r\n    </div>\r\n\r\n    <!-- 新增/编辑对话框 -->\r\n    <el-dialog title=\"实验室预约\" :visible.sync=\"fromVisible\" width=\"40%\" :close-on-click-modal=\"false\" destroy-on-close>\r\n      <el-form :model=\"form\" label-width=\"100px\" style=\"padding-right: 50px\" :rules=\"rules\" ref=\"formRef\">\r\n        <!-- 表单项 -->\r\n        <el-form-item label=\"实验室编号\" prop=\"labId\">\r\n          <el-select v-model=\"form.labId\" placeholder=\"请选择实验室编号\" style=\"width: 100%\">\r\n            <el-option  v-for=\"item in labData\" :label=\"item.number\" :value=\"item.id\"></el-option>\r\n          </el-select>\r\n        </el-form-item>\r\n        <el-form-item label=\"借用学期\" prop=\"semesterName\">\r\n          <el-input :value=\"curSemester.name\" disabled></el-input> <!-- 显示当前学期，禁止编辑 -->\r\n        </el-form-item>\r\n        <el-form-item label=\"周次\" prop=\"week\">\r\n          <el-input v-model=\"form.week\" placeholder=\"周次\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"星期\" prop=\"day\">\r\n          <el-select v-model=\"form.day\" placeholder=\"星期\" style=\"width: 100%\">\r\n            <el-option  label=\"周一\" value=\"周一\"></el-option>\r\n            <el-option  label=\"周二\" value=\"周二\"></el-option>\r\n            <el-option  label=\"周三\" value=\"周三\"></el-option>\r\n            <el-option  label=\"周四\" value=\"周四\"></el-option>\r\n            <el-option  label=\"周五\" value=\"周五\"></el-option>\r\n            <el-option  label=\"周六\" value=\"周六\"></el-option>\r\n            <el-option  label=\"周日\" value=\"周日\"></el-option>\r\n          </el-select>\r\n        </el-form-item>\r\n        <el-form-item label=\"节次\" prop=\"sessionId\">\r\n          <el-select v-model=\"form.sessionId\" placeholder=\"请选择节次\" style=\"width: 100%\">\r\n            <el-option  v-for=\"item in sessions\" :label=\"item.name\" :value=\"item.id\"></el-option>\r\n          </el-select>\r\n        </el-form-item>\r\n        <el-form-item label=\"申请学生\" prop=\"studentName\">\r\n          <el-input v-model=\"user.name\" placeholder=\"申请学生\" disabled></el-input> <!-- 显示当前登录学生，禁止编辑 -->\r\n        </el-form-item>\r\n        <el-form-item label=\"申请原因\" prop=\"reason\">\r\n          <el-input v-model=\"form.reason\" placeholder=\"申请原因\"></el-input>\r\n        </el-form-item>\r\n      </el-form>\r\n\r\n      <!-- 对话框底部操作按钮 -->\r\n      <div slot=\"footer\" class=\"dialog-footer\">\r\n        <el-button @click=\"fromVisible = false\">取 消</el-button>\r\n        <el-button type=\"primary\" @click=\"save\">确 定</el-button>\r\n      </div>\r\n    </el-dialog>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: \"Borrow\",\r\n  data() {\r\n    return {\r\n      tableData: [],  // 表格数据\r\n      pageNum: 1,   // 当前页码\r\n      pageSize: 10,  // 每页显示的条数\r\n      total: 0,  // 总记录数\r\n      fromVisible: false, // 控制对话框显示\r\n      form: {},  // 表单数据\r\n      user: JSON.parse(localStorage.getItem('xm-user') || '{}'), // 当前用户信息\r\n      curSemester: null, // 当前学期信息\r\n      rules: { // 表单验证规则\r\n        labId: [{ required: true, message: '请选择实验室编号', trigger: 'blur' }],\r\n        week: [{ required: true, message: '请选择借用周', trigger: 'blur' }],\r\n        day: [{ required: true, message: '请选择借用星期', trigger: 'blur' }],\r\n        sessionId: [{ required: true, message: '请选择借用节次', trigger: 'blur' }],\r\n        reason: [{ required: true, message: '请输入借用原因', trigger: 'blur' }]\r\n      },\r\n      labData: [], // 实验室数据\r\n      sessions: [], // 节次数据\r\n      headFilters: [] // 筛选器配置\r\n    };\r\n  },\r\n  created() {\r\n    // 页面加载时初始化数据\r\n    this.load(1); // 加载表格数据\r\n    this.loadLab(); // 加载实验室信息\r\n    this.loadCurSemester(); // 加载当前学期信息\r\n    this.loadSessions(); // 加载节次信息\r\n  },\r\n  methods: {\r\n    load(pageNum) {\r\n      // 分页加载数据\r\n      if (pageNum) this.pageNum = pageNum;\r\n      this.$request.get('/borrow/selectPage', {\r\n        params: { pageNum: this.pageNum, pageSize: this.pageSize }\r\n      }).then(res => {\r\n        if (res.code === '200') {\r\n          this.tableData = res.data?.list;\r\n          this.total = res.data?.total;\r\n        }\r\n      });\r\n    },\r\n    handleAdd() {\r\n      // 打开新增借用申请对话框\r\n      this.form = {};\r\n      this.fromVisible = true;\r\n    },\r\n    save() {\r\n      // 保存数据\r\n      this.$refs.formRef.validate(valid => {\r\n        if (valid) {\r\n          this.$request.post('/borrow/add', this.form).then(res => {\r\n            if (res.code === '200') {\r\n              this.$message.success('保存成功');\r\n              this.load(1);\r\n              this.fromVisible = false;\r\n            }\r\n          });\r\n        }\r\n      });\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n/* 自定义样式 */\r\n</style>\r\n"],"mappings":"AAgGA;EACAA,IAAA;EACAC,KAAA;IACA;MACAC,SAAA;MAAA;MACAC,OAAA;MAAA;MACAC,QAAA;MAAA;MACAC,KAAA;MAAA;MACAC,WAAA;MAAA;MACAC,IAAA;MAAA;MACAC,IAAA,EAAAC,IAAA,CAAAC,KAAA,CAAAC,YAAA,CAAAC,OAAA;MAAA;MACAC,WAAA;MAAA;MACAC,KAAA;QAAA;QACAC,KAAA;UAAAC,QAAA;UAAAC,OAAA;UAAAC,OAAA;QAAA;QACAC,IAAA;UAAAH,QAAA;UAAAC,OAAA;UAAAC,OAAA;QAAA;QACAE,GAAA;UAAAJ,QAAA;UAAAC,OAAA;UAAAC,OAAA;QAAA;QACAG,SAAA;UAAAL,QAAA;UAAAC,OAAA;UAAAC,OAAA;QAAA;QACAI,MAAA;UAAAN,QAAA;UAAAC,OAAA;UAAAC,OAAA;QAAA;MACA;MACAK,OAAA;MAAA;MACAC,QAAA;MAAA;MACAC,WAAA;IACA;EACA;EACAC,QAAA;IACA;IACA,KAAAC,IAAA;IACA,KAAAC,OAAA;IACA,KAAAC,eAAA;IACA,KAAAC,YAAA;EACA;EACAC,OAAA;IACAJ,KAAAxB,OAAA;MACA;MACA,IAAAA,OAAA,OAAAA,OAAA,GAAAA,OAAA;MACA,KAAA6B,QAAA,CAAAC,GAAA;QACAC,MAAA;UAAA/B,OAAA,OAAAA,OAAA;UAAAC,QAAA,OAAAA;QAAA;MACA,GAAA+B,IAAA,CAAAC,GAAA;QACA,IAAAA,GAAA,CAAAC,IAAA;UACA,KAAAnC,SAAA,GAAAkC,GAAA,CAAAnC,IAAA,EAAAqC,IAAA;UACA,KAAAjC,KAAA,GAAA+B,GAAA,CAAAnC,IAAA,EAAAI,KAAA;QACA;MACA;IACA;IACAkC,UAAA;MACA;MACA,KAAAhC,IAAA;MACA,KAAAD,WAAA;IACA;IACAkC,KAAA;MACA;MACA,KAAAC,KAAA,CAAAC,OAAA,CAAAC,QAAA,CAAAC,KAAA;QACA,IAAAA,KAAA;UACA,KAAAZ,QAAA,CAAAa,IAAA,qBAAAtC,IAAA,EAAA4B,IAAA,CAAAC,GAAA;YACA,IAAAA,GAAA,CAAAC,IAAA;cACA,KAAAS,QAAA,CAAAC,OAAA;cACA,KAAApB,IAAA;cACA,KAAArB,WAAA;YACA;UACA;QACA;MACA;IACA;EACA;AACA","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}