{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nexport default {\n  name: \"Login\",\n  data() {\n    return {\n      form: {},\n      // 表单数据\n      rules: {\n        // 表单校验规则\n        username: [{\n          required: true,\n          message: '请输入账号',\n          trigger: 'blur'\n        }],\n        password: [{\n          required: true,\n          message: '请输入密码',\n          trigger: 'blur'\n        }]\n      }\n    };\n  },\n  methods: {\n    login() {\n      // 表单校验\n      this.$refs['formRef'].validate(valid => {\n        if (valid) {\n          // 如果表单验证通过，发送登录请求\n          this.$request.post('/login', this.form).then(res => {\n            if (res.code === '200') {\n              // 登录成功\n              localStorage.setItem(\"xm-user\", JSON.stringify(res.data)); // 将用户数据存储到本地\n              this.$router.push('/'); // 跳转到主页\n              this.$message.success('登录成功');\n            } else {\n              // 登录失败\n              this.$message.error(res.msg);\n            }\n          });\n        }\n      });\n    }\n  }\n};","map":{"version":3,"names":["name","data","form","rules","username","required","message","trigger","password","methods","login","$refs","validate","valid","$request","post","then","res","code","localStorage","setItem","JSON","stringify","$router","push","$message","success","error","msg"],"sources":["src/views/Login.vue"],"sourcesContent":["<template>\r\n  <div class=\"container\">\r\n    <!-- 登录框 -->\r\n    <div style=\"width: 400px; padding: 30px; background-color: white; border-radius: 5px;\">\r\n      <!-- 标题 -->\r\n      <div style=\"text-align: center; font-size: 20px; margin-bottom: 20px; color: #333\">\r\n        欢迎登录计算机工程学院实验室管理系统\r\n      </div>\r\n      <!-- 表单 -->\r\n      <el-form :model=\"form\" :rules=\"rules\" ref=\"formRef\">\r\n        <!-- 用户名 -->\r\n        <el-form-item prop=\"username\">\r\n          <el-input prefix-icon=\"el-icon-user\" placeholder=\"请输入账号\" v-model=\"form.username\"></el-input>\r\n        </el-form-item>\r\n        <!-- 密码 -->\r\n        <el-form-item prop=\"password\">\r\n          <el-input prefix-icon=\"el-icon-lock\" placeholder=\"请输入密码\" show-password v-model=\"form.password\"></el-input>\r\n        </el-form-item>\r\n        <!-- 角色选择 -->\r\n        <el-form-item>\r\n          <el-select v-model=\"form.role\" placeholder=\"请选择角色\" style=\"width: 100%\">\r\n            <el-option label=\"系统管理员\" value=\"ADMIN\" />\r\n            <el-option label=\"实验员\" value=\"LABADMIN\" />\r\n            <el-option label=\"教师\" value=\"TEACHER\" />\r\n            <el-option label=\"学生\" value=\"STUDENT\" />\r\n          </el-select>\r\n        </el-form-item>\r\n        <!-- 登录按钮 -->\r\n        <el-form-item>\r\n          <el-button\r\n              style=\"width: 100%; background-color: #0376bf; border-color: #0376bf; color: white\"\r\n              @click=\"login\"\r\n          >\r\n            登 录\r\n          </el-button>\r\n        </el-form-item>\r\n      </el-form>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n\r\n<script>\r\nexport default {\r\n  name: \"Login\",\r\n  data() {\r\n    return {\r\n      form: {}, // 表单数据\r\n      rules: { // 表单校验规则\r\n        username: [\r\n          { required: true, message: '请输入账号', trigger: 'blur' },\r\n        ],\r\n        password: [\r\n          { required: true, message: '请输入密码', trigger: 'blur' },\r\n        ],\r\n      },\r\n    };\r\n  },\r\n  methods: {\r\n    login() {\r\n      // 表单校验\r\n      this.$refs['formRef'].validate((valid) => {\r\n        if (valid) {\r\n          // 如果表单验证通过，发送登录请求\r\n          this.$request.post('/login', this.form).then(res => {\r\n            if (res.code === '200') {\r\n              // 登录成功\r\n              localStorage.setItem(\"xm-user\", JSON.stringify(res.data)); // 将用户数据存储到本地\r\n              this.$router.push('/'); // 跳转到主页\r\n              this.$message.success('登录成功');\r\n            } else {\r\n              // 登录失败\r\n              this.$message.erro